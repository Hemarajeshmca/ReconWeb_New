	function RuleApplied() {
		if ($("#RuleAppliedOn").val() == 'Transaction') {
			$("#Transaction1").show();
			$("#Transaction2").show();
			$("#Transaction3").show();
			$("#Supporting1").hide();
			$("#Supporting2").hide();
			$("#Supporting3").hide();
