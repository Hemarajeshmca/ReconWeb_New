			$("#Transaction3").hide();
			$("#Supporting1").show();
			$("#Supporting2").show();
			$("#Supporting3").show();
		}
	}
	function flag() {
		var flag = $('input[name="Groupflag"]:checked').val()
		if (flag == "Y") {
			$("#grouping").show();
		} else {
			$("#grouping").hide();
		}
	}
	function validateNextrule() {
		$("#rule").removeClass('nav-link active');
