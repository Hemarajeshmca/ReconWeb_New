<script type="text/javascript">
	$(document).ready(function () {
		$("#navhead").text("File Template Details");
		$("#csvtab").hide();
		$("#status_excel").hide();
		$("#csvFieldName").hide();
	});

	function tempTypeChange() {
		if ($("#tempType").val() == 'excel') {
			$("#exceltab").show();
			$("#csvtab").hide();
		} else {
			$("#exceltab").hide();
			$("#csvtab").show()
		}
	}

	function fileTypeChange() {
		debugger;
		if ($("#fileType").val() == 1 || $("#fileType").val() == 3) {
			$("#status_excel").show();
			$("#map_field").hide();
			$("#mapcsvFieldName").hide();
			$("#csvFieldName").show();
		} else {
			$("#status_excel").hide();
			$("#map_field").show();
			$("#mapcsvFieldName").show();
			$("#csvFieldName").hide();
		}
	}

	function rr3() {
		$("#viewExcel").show();
	}

	function closeExel() {
		$("#viewExcel").hide();
	}

	function rr4() {
		$("#viewCSV").show();
		fileTypeChange();
	}

	function closeCSV() {
		$("#viewCSV").hide();
	}

</script>

